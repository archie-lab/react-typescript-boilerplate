{
  "name": "react-typescript-boilerplate",
  "version": "1.0.0",
  "description": "React boilerplate with TypeScript",
  "main": "index.ts",
  "scripts": {
    "prebuild": "npm run build:clean",
    "precommit": "npm run eslint",
    "pretest": "npm run test:clean && npm run eslint",
    "build:clean": "rimraf ./dist",
    "clean": "npm run test:clean && npm run build:clean",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.config.js --mode development --hot --max-old-space-size=4096",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.config.js --mode production --progress --profile --colors",
    "eslint": "eslint --ignore-path .gitignore --color --ext .ts,.tsx,.js,.jsx src",
    "eslint:fix": "eslint --ignore-path .gitignore --color --ext .ts,.tsx,.js,.jsx src --fix",
    "storybook": "start-storybook -p 9001 -c config/.storybook",
    "test": "jest --coverage",
    "test:clean": "rimraf ./coverage",
    "test:watch": "jest --watchAll"
  },
  "author": "Archie",
  "license": "MIT",
  "keywords": [
    "React",
    "typescript",
    "redux",
    "redux-saga",
    "webpack",
    "pwa",
    "ES6"
  ],
